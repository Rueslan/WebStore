@model WebStore.Models.Employee

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>CurrentEmployee</title>
</head>
<body>
    <h3>Информация о сотруднике</h3>

    <table>
        <thead>
            <tr>
                <th>#</th>
                <th>Фамилия</th>
                <th>Имя</th>
                <th>Отчество</th>
                <th>Возраст</th>
                <th>Выслуга лет</th>
            </tr>
        </thead>
        <tbody>
            <tr> 
                <td>@Model.id</td>
                <td>@Model.LastName</td>
                <td>@Model.FirstName</td>
                <td>@Model.Patronymic</td>
                <td>@Model.Age</td>
                <td>@Model.Seniority</td>
            </tr>
        </tbody>
    </table>
    <br/>
    <p>
        <a href="/Home/Employees">Назад</a>
    </p>
</body>
</html>
